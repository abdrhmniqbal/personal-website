{
  "name": "personal",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun build:content && bun build:client",
    "build:content": "velite --clean",
    "build:client": "react-router build",
    "dev": "concurrently \"bun:dev:*\"",
    "dev:content": "velite --watch",
    "dev:client": "react-router dev",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx,md,js,jsx,json,css}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx,md,js,jsx,json,css}\" --cache",
    "lint": "eslint ./app ./server --cache",
    "lint:fix": "eslint ./app ./server --fix"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@epic-web/client-hints": "^1.3.5",
    "@epic-web/invariant": "^1.0.0",
    "@forge42/seo-tools": "^1.3.0",
    "@react-router/cloudflare": "^7.0.2",
    "@tailwindcss/vite": "^4.0.0-beta.8",
    "@unpic/react": "^0.1.15",
    "concurrently": "^9.1.0",
    "framer-motion": "^11.13.1",
    "hono": "^4.6.12",
    "hugeicons-react": "^0.3.0",
    "isbot": "^4.4.0",
    "react": "^19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.0.2",
    "react-router-dom": "^7.0.2",
    "react-router-hono-server": "^2.2.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.7",
    "@react-router/dev": "^7.0.2",
    "@react-router/remix-routes-option-adapter": "^7.0.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitest/eslint-plugin": "^1.1.17",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^19.0.0-beta-201e55d-20241215",
    "eslint": "^9.16.0",
    "eslint-plugin-import-x": "^4.5.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-compiler": "^19.0.0-beta-201e55d-20241215",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^15.13.0",
    "miniflare": "^3.20241106.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-router-devtools": "^1.0.5",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.0",
    "rehype-slug": "^6.0.0",
    "remix-flat-routes": "^0.6.5",
    "tailwindcss": "^4.0.0-beta.8",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "velite": "^0.2.1",
    "vite": "^5.4.11",
    "vite-plugin-babel": "^1.3.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "3.81.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
