{
  "name": "personal",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun build:content && bun build:client",
    "build:content": "velite --clean",
    "build:client": "react-router build",
    "dev": "concurrently \"bun:dev:*\"",
    "dev:content": "velite --watch",
    "dev:client": "react-router dev",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx,md,js,jsx,json,css}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx,md,js,jsx,json,css}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@forge42/seo-tools": "^1.3.0",
    "@react-router/cloudflare": "^7.0.2",
    "@unpic/react": "^0.1.15",
    "concurrently": "^9.1.0",
    "framer-motion": "^11.13.1",
    "hono": "^4.6.12",
    "hugeicons-react": "^0.3.0",
    "isbot": "^4.4.0",
    "react": "^18.3.1",
    "react-aria-components": "^1.5.0",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.2",
    "react-router-dom": "^7.0.2",
    "react-router-hono-server": "^2.2.1",
    "remix-themes": "^1.5.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^1.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@epic-web/config": "^1.16.4",
    "@hono/node-server": "^1.13.7",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@react-router/dev": "^7.0.2",
    "@react-router/remix-routes-option-adapter": "^7.0.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "miniflare": "^3.20241106.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-router-devtools": "^1.0.5",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.0",
    "rehype-slug": "^6.0.0",
    "remix-flat-routes": "^0.6.5",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "velite": "^0.2.1",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "3.81.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
